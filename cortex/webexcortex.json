{
    "name": "Webex-Cortex",
    "version": "0.1",
    "author": "Kalle M. Aagaard",
    "url": "https://github.com/KalleDK/hive-webexteams",
    "license": "MIT",
    "description": "Manage rooms in Webex Teams",
    "dataTypeList": ["thehive:case"],
    "command": "webexcortex",
    "baseConfig": "WebexTeams",
    "configurationItems": [
      {
        "name": "organization",
        "description": "Short name for organization, to be used in WebEx room title.",
        "type": "string",
        "multi": false,
        "required": true,
        "default": "ORG"
      },
      {
        "name": "webex_bot_token",
        "description": "Token for the webex bot",
        "type": "string",
        "multi": false,
        "required": true,
        "default": "XXXXXXX"
      }
    ]
}
